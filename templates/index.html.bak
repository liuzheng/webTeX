<!DOCTYPE html>
<html ng-app>
<head lang="en">
    <meta charset="UTF-8">
    <title>大玉螺旋丸</title>
    <link rel="stylesheet" type="text/css" href="css/bootstrap.min.css"/>
    <link rel="stylesheet" type="text/css" href="css/bootstrap-theme.min.css"/>
    <link rel="stylesheet" type="text/css" href="css/dashboard.css"/>
    <script src="js/jquery.min.js"></script>
    <script src="js/angular.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script>
        function naldo($scope) {
            $scope.link = [
                'http://master/dists/',
                'http://master/phpmyadmin/',
                'http://master:50070/',
                'http://master:60010/',
                'http://master:8080/',
                'http://www.ilz.me:8112'
            ];
            $scope.name = [
                'Ubuntu 镜像',
                'PHPMyAdmin',
                'Hadoop',
                'Hbase',
                'Spark',
                'Deluge'
            ];
            $scope.L = Array.apply(null, Array($scope.name.length)).map(function (_, i) {
                return i;
            });
            $scope.t = 0;
            $scope.source = $scope.link[$scope.t];
            document.getElementById('iframe1').src = $scope.source;
            $scope.Click = function (t) {
                $scope.t = t;
                $scope.source = $scope.link[$scope.t];
                document.getElementById('iframe1').src = $scope.source
            }
        }
    </script>
</head>
<body class="container">
<nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar"
                    aria-expanded="false" aria-controls="navbar">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="#">总控制台</a>
        </div>
        <div id="navbar" class="collapse navbar-collapse">
            <ul class="nav navbar-nav">
                <li class="active"><a href="#">Home</a></li>
                <li><a href="#about">About</a></li>
                <li><a href="#contact">Contact</a></li>
            </ul>
        </div>
        <!--/.nav-collapse -->
    </div>
</nav>

<div ng-controller="naldo">
    <div class="col-md-2 col-lg-2 sidebar">
        <ul class="nav nav-sidebar" ng-repeat="m in L">
            <li ng-class="{true:'active',false:''}[t==m]"><a ng-click="Click(m)" href="">{{name[m]}}</a></li>
        </ul>
    </div>
    <div class="col-sm-12 col-sm-offset-1 col-md-11 col-md-offset-2 col-lg-12 col-lg-offset-1" style="height: 100%">
        <IFRAME id='iframe1' width="100%" height="93%"></IFRAME>
    </div>
</div>
</body>
</html>
